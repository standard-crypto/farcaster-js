    "eslint-plugin-promise": "^6.1.0",
    "esm": "^3.2.25",
    "fix-esm-import-path": "^1.4.0",
    "husky": "^8.0.3",
    "lerna": "^5.2.0",
    "markdown-magic": "^2.6.1",
    "pinst": "^3.0.0",
    "prettier": "^3.0.3",
    "semantic-release": "^22.0.5",
    "source-map-support": "^0.5.19",
    "tshy": "1.5.0",
    "typescript": "^5.2.2"
